{
    "name": "ddaniel/gh-releases",
    "description": "template for github releases with ci/cd",
    "version": "1.4.0",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ddaniel\\GhReleases\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Daniel Dubchenko"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
